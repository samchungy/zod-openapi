{
  "name": "zod-openapi",
  "version": "0.5.0",
  "description": "A library to create full OpenAPI documents from your Zod types",
  "homepage": "https://github.com/samchungy/zod-openapi#readme",
  "bugs": {
    "url": "https://github.com/samchungy/zod-openapi/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/samchungy/zod-openapi.git"
  },
  "license": "MIT",
  "sideEffects": false,
  "main": "./lib-commonjs/index.js",
  "module": "./lib-es2015/index.js",
  "types": "./lib-types/index.d.ts",
  "files": [
    "lib*/**/*.d.ts",
    "lib*/**/*.js",
    "lib*/**/*.js.map",
    "lib*/**/*.json"
  ],
  "scripts": {
    "build": "skuba build-package",
    "commit": "cz",
    "format": "skuba format",
    "lint": "skuba lint",
    "release": "yarn build && npm publish",
    "test": "skuba test",
    "test:ci": "skuba test --coverage",
    "test:watch": "skuba test --watch"
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  },
  "dependencies": {
    "openapi3-ts": "4.0.4",
    "yaml": "2.2.1"
  },
  "devDependencies": {
    "@types/node": "^18.15.11",
    "commitizen": "^4.2.4",
    "skuba": "6.0.2",
    "zod": "3.21.4"
  },
  "peerDependencies": {
    "zod": "^3.21.4"
  },
  "engines": {
    "node": ">=16.11"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org/"
  },
  "skuba": {
    "entryPoint": "src/index.ts",
    "template": "oss-npm-package",
    "type": "package",
    "version": "6.0.0"
  }
}
